        } 
        if ((this.p13 != 0) && (this.p13 == this.p22) && (this.p22 == this.p31)) {
            toReturn = this.p13;
        } if (this.p11 * this.p12 * this.p13 * this.p21 * this.p22 * this.p23 * this.p31 * this.p32 * this.p33 != 0) {
            toReturn = 3;
        }
        return toReturn;

    }

